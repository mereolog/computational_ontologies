(cl-text http://colore.oor.net/periods/infinite_periods.clif

(cl-imports http://colore.oor.net/periods/periods.clif)

(cl-comment 'SUCC-1')
(forall (x)
                (exists (y)
                                (precedence x y)))

(cl-comment 'SUCC-2')
(forall (x)
                (exists (y)
                                (precedence y x)))


)
