(cl-text http://colore.oor.net/magma/group.clif

(cl-imports http://colore.oor.net/magma/magma.clif)

(cl-imports http://colore.oor.net/magma/definitions/magma_defs.clif)

(forall (x y z)
	(= (op x (op y z)) (op (op x y) z)))

(exists x
	(identity x))

(forall (x)
	(exists (y)
		(group_inverse x y)))

)
