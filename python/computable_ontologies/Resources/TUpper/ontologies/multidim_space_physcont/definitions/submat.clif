(cl-text http://colore.oor.net/multidim_space_physcont/definitions/submat.clif

(cl-imports http://colore.oor.net/multidim_space_physcont/definitions/matcont.clif)


(cl-comment 'SubMat-D')

(forall (x y)
	(iff
		(submaterial x y)
		(and
			(matcont y x)
			(P (r x) (r y))
		)
	)
)


)