(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/quadripartite_incidence/double_mapping.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/quadripartite_incidence/weak_quadripartite.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/bipartite_incidence/mapping_bipartite.clif)

(forall (q)
	(if	(plane q)
		(exists (s)
			(and	(space s)
				(in q s)))))

(forall (s1 s2 q)
	(if	(and	(space s1)
			(space s2)
			(plane q)
			(in q s1)
			(in q s2))
		(= s1 s2)))

(forall (x y)
	(if	(and	(in x y)
			(not (= x y)))
		(iff	(point x)
			(line y))))

(forall (x y)
	(if	(and	(in x y)
			(not (= x y)))
		(iff	(plane x)
			(space y))))

)
