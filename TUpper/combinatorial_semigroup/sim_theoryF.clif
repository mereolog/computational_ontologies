(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/combinatorial_semigroup/sim_theoryF.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/combinatorial_semigroup/semigroup_gen.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/magma/cancellative.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/magma/definitions/magma_defs.clif)

(forall (x y z w)
	(if	(and	(generator x)
			(generator y))
		(not (= (op w x) (op z y)))))

(forall (x)
	(if	(not (or	(identity x)
				(generator x)))
		(exists (u y)
			(and	(generator y)
				(= (op u y) x)))))

)
