(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/combinatorial_semigroup/semigroup_gen.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/magma/semigroup.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/magma/definitions/magma_defs.clif)

(forall (x y z)
        (if     (and    (generator x)
                        (= x (op y z)))
                (or     (identity y)
                        (identity z))))

(forall (x y z)
	(if	(and	(generator x)
			(= (op y x) (op z x)))
		(= y z)))

(forall (x y z)
	(if	(and	(generator y)
			(generator z)
			(= (op x y) (op x z)))
		(= y z)))

(forall (x y)
        (if     (or	(= (op x y) y)
			(= (op y x) y))
		(not (generator x))))

)
