(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/combinatorial_semigroup/free_semigroup_fo.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/combinatorial_semigroup/tame_theoryF.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/combinatorial_semigroup/thin_TC.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/combinatorial_semigroup/definitions/words_def.clif)

(forall (x y)
	(if	(subword x y)
		(exists (z)
			(and	(initial_subword z y)
				(not (subword x z))
				(initial_subword (op z x) y)))))

)
