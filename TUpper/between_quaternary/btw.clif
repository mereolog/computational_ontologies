(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between_quaternary/btw.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between_quaternary/btw_basic.clif)

(cl-comment 'basic axioms of quaternary betweenness extended by separability')


(cl-comment 'B-A6: separation property')

(forall (r a b c x)
	(if
		(and
			(Btw r a x b)
			(Btw r a x c)
		)
		(not (Btw r b x c))
	)
)

)