(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/between_quaternary/btw_semilinear.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/between_quaternary/btw_root.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/between_quaternary/definitions/incidence.clif)

(forall (l x y z w)
	(if	
        (and
            (Btw l y x z)
            (In w l)
        )
		(or	
            (Btw l y x w)
			(Btw l z x w)
        )
    )
)

)
