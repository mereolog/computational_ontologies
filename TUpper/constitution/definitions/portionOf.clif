(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/matter/definitions/portionOf.clif
  
(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/matter/matter.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/constitution/constitution.clif)

(forall (x y)
        (iff    (portionOf x y)
                (exists (w z)
                        (and    (constitutes w x)
                                (constitutes z y)
                                (chunkOf w z)))))

