(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/simple_features/sfr_fol/theorems/sf_crosses_theorems.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/simple_features/sfr_fol/sf_crosses.clif)


(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/simple_features/sfc_core/sf_curve.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_codi/definitions/inc.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_codi/definitions/sc.clif)


(cl-comment 'SFR-T3')

(forall (x y)
	(if	
		(and
			(< x y)
			(sf_crosses x y)
		)
		(and
			(Inc x y)
			(not (Cont x y))
		)
	)
)


(cl-comment 'SFR-T4')


(forall (x y)
	(if	
		(and
			(EqDim x y)
			(sf_crosses x y)
		)
		(SC x y)
	)
)

(cl-comment 'SFR-T4')

(forall (x y)
	(if	
		(and
			(sf_crosses x y)
			(sf_curve x)
			(sf_curve y)
		)
		(SC x y)
	)
)


)