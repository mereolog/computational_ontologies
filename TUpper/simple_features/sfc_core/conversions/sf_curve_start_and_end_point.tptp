fof(axiom20, axiom, (! [X] : ! [Y] :  ((sf_start_point(x,y) -> (sf_point(x) & sf_curve(y)))))).
fof(axiom30, axiom, (! [X] : ! [Y] :  ((sf_end_point(x,y) -> (sf_point(x) & sf_curve(y)))))).
fof(axiom40, axiom, (! [X] :  ((sf_curve(x) -> \exists p1,p2\;[(sf_start_point(p1,x) & sf_end_point(p2,x))])))).
