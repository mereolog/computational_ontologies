(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/simple_features/sfc_core/sf_point.clif


(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/simple_features/sfc_core/definitions/sf_geometry.clif)


(cl-comment 'SFC-A3')
(forall (x)
	(if
		(sf_point x)
		(and
			(not (sf_curve x))
			(not (sf_surface x))
			(not (sf_geometry_collection x))
		)
	)
	
)
)