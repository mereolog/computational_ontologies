(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/simple_features/sfc_fol/sfc_fol_multi_line_string.clif

(cl-imports  https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/simple_features/sfc_fol/sfc_fol_multi_curve.clif)

(cl-imports  https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/simple_features/sfc_core/sf_line_string.clif)

(cl-comment 'SFC-M17')
(forall (x)
	(iff
		(sf_multi_line_string x)
		(and	
			(sf_multi_curve x)
			(forall (y)
				(if
					(and
						(P y x)
						(Min y)
					)
					(sf_line_string y)
				)
			)
		)
	)	
)


)