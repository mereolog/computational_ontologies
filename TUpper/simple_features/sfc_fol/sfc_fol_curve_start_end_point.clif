(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/simple_features/sfc_fol/sfc_fol_curve_start_and_end_point.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/simple_features/sfc_core/sf_curve_start_and_end_point.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codib/codi_bcont.clif)

(cl-comment 'Simple Feature Start Point is boundary contained in the curve')
(forall (x y)
	(if	
		(sf_start_point x y)
		(BCont x y)
	)
)

(cl-comment 'Simple Feature End Point is boundary contained in the curve')
(forall (x y)
	(if	
		(sf_end_point x y)
		(BCont x y)
	)
)


)
