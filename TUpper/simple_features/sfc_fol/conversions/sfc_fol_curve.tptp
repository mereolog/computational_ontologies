fof(axiom220, axiom, (! [X] :  ((sf_curve(x) <-> (SimpleCurveSegment(x) | SimpleLoopCurve(x)))))).
fof(axiom230, axiom, (! [X] :  (((sf_curve(x) & SimpleCurveSegment(x)) -> \exists p1,p2\;[(sf_point(p1) & sf_point(p2) & sf_start_point(p1,x) & sf_end_point(p2,x) & BCont(p1,x) & BCont(p2,x) & ~=(p1,p2))])))).
fof(axiom240, axiom, (! [X] :  (((sf_curve(x) & SimpleLoopCurve(x)) -> (\exists p1,p2\;[(sf_point(p1) & sf_point(p2) & sf_start_point(p1,x) & sf_end_point(p2,x))] -> =(y,z)))))).
