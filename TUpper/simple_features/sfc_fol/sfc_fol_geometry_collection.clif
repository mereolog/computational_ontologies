(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/simple_features/sfc_core/sf_geometry_collection.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/simple_features/sfc_core/definitions/sf_geometry.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/multipart_s.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/branched_s.clif)


(cl-comment 'SFC-M13: sf_geometry_collection is a specialization of either CODIB multipart or a branched spatial region -- this implies that a collection must contain multiple disconnected or branching entities, which captures its intent but is never declared in the  SFA standard as such')

(forall (x)
	(if
		(sf_geometry_collection x)
		(or
			(Multipart_S x)
			(Branched_S x)
		)
	)
	
)

