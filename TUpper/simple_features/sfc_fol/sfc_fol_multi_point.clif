(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/simple_features/sfc_fol/sfc_fol_multi_point.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/simple_features/sfc_core/sf_geometry_collection.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/point_region.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/point.clif)

(cl-comment 'SFC-M14: Simple Feature Multipoint is a PointRegion that is not a point')


(forall (x)
	(iff	
		(sf_multi_point x)
		(and
			(PointRegion x)
			(not (Point x))
		)
	)
)



(cl-comment 'SFC-M14 (old): Simple Feature Multipoint and PointRegion are equivalent classes - this is inconsistent with the approach that a geometry collection, which includes multipoints, is necessarily a multipart or branched areal region') 


)