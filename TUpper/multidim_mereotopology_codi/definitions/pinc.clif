(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_mereotopology_codi/definitions/pinc.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_mereotopology_codi/definitions/inc.clif)

(cl-comment 'PInc-D: proper incidence is incidence where neither of the involved entity is contained in the other.')

(forall (x y)
	(iff	
		(PInc x y)
		(and
			(Inc x y)
			(not (Cont x y))
			(not (Cont y x))
		)
	)
)

)


