(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between/bet_du.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between/reflexive_bet.clif)

(cl-comment ‘Betweenness and Comparability Obtained from Binary Relations’)

(cl-comment ‘Section 4 Betweenness relations, BT0 - BT3’)
(cl-comment ‘BT0 & BT1 are in reflexive_bet.clif’)

(cl-comment ‘BT2’)
(forall (a b c)
	(if	(between a b c)
		(between a a b)))

(cl-comment ‘BT3’)
(forall (a b c)
	(if	(and	(between a b c)
			(between a c b))
		(= b c)))

)

