(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/between/betweenness.clif

(forall (x y z)
	(if	(between x y z)
		(between z y x)))

(forall (x y z)
	(if	(and	(between x y z)
			(between y x z))
		(= x y)))

)
