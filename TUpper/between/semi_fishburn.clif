(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between/semi_fishburn.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between/fishburn.clif)
(cl-comment ‘from Interval Orders and Graphs, page 57-60’)

(cl-comment ‘A4’)
(forall (x y z a b c)
	(if	(and	(between a b c)
			(between x y z))
		(or	(between a b x)
			(between a b z)
			(between x b c)
			(between z b c))))

(cl-comment ‘A5’)				
(forall (x a b c d)
	(if	(and	(between a b c)
			(between b c d)
			(not	(between a b x)))
		(or	(between x c d)
			(between a x d))))

)
