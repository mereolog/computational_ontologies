(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between/propert_c_set.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between/c_set_between.clif)

(forall (b c)
	(exists (a)
		(between b a c)))

(forall (a b)
	(if	(not (= a b))
		(exists (c)
			(and	(not (= b c))
				(between b a c)))))

)
