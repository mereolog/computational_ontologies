(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between/ps_distributive.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/between/ps_lattice.clif)
(cl-comment ‘Transitivities of Betweenness’)
(cl-comment ‘Interpretations of certain of the five point transitivities fro lattice betweenness’)


(cl-comment ‘distributive’) 
(cl-comment ‘Theorem 9.3 iff a lattice L is distributive, has transitivity T5’)
(forall (a b c d x)
	(if	(and	(between a b c)
			(between a d c)
			(between b x d))
		(between a x c)))

(cl-comment ‘iff L is distributive, has transitivities T4 and T7’) 
(cl-comment ‘T4’)
(forall (a b c d x)
	(if	(and	(between a b c)
			(between d a b)
			(between x c d)
		(between a b x)))

(cl-comment ‘T7’)
(forall (a b c d x)
	(if	(and	(between a b c)
			(between a d b)
			(between c x d)
		(between a b x)))

)
