(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/between/semilinear.clif

(forall (x y z)
	(if	(between x y z)
		(between z y x)))

(forall (x y z)
	(iff	(and	(between y x z)
			(between x y z))
		(= x y)))

(forall (x y z w)
	(if	(between y x z)
		(or	(between y x w)
			(between z x w))))

)
