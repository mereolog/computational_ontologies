(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/between/weak_fishburn.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/between/semi_fishburn.clif)

(forall (x y z a)
	(if	(and	(between x y z)
			(not	(between a y z)))
		(or	(between x a z)
			(between x y a))))

)