(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/between/convexity.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/between/weak_between.clif)

(forall (x y z w v)
	(if	(and	(between x y z)
			(between x w z)
			(between y v w))
		(between x v z)))

)

