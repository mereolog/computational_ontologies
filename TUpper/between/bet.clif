(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/between/bet.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/between/weak_between.clif)

(forall (x y z w)
        (if     (and    (between x y z)
			(not (= y z))
                        (between y z w))
                (between x y w)))

(forall (x y z w)
        (if     (and    (between x y w)
                        (between y z w))
                (between x y z)))

)

