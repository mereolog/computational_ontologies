(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/most/most_ringbond.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/most/definitions/most_group_definitions.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/most/most_betweenness.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/most/definitions/most_ringbond_definitions.clif)


(cl-comment 'Axiom: MRB-1')
(cl-comment 'If a bond is a ringbond, then it must be in a ring. change commentd')
(forall (b)
	(if (ringBond b)
		(exists (g)
			(and (mol b g)(ring g)))))
		

)