(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/most/incidence_quad_td.clif

(forall (x y)
	(iff (inpl x y)
		(and (in x y)(point x)(line y))))

(forall (x y)
	(iff (inpq x y)
		(and (in x y)(point x)(plane y))))
		
(forall (x y)
	(iff (inlq x y)
		(and (in x y)(line x)(plane y))))
		
(forall (x y)
	(iff (inps x y)
		(and (in x y)(point x)(space y))))
		
(forall (x y)
	(iff (inls x y)
		(and (in x y)(line x)(space y))))

(forall (x y)
	(iff (inqs x y)
		(and (in x y)(plane x)(space y))))

)