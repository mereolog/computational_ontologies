(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/most/mappings/most_root_reduction.clif

(cl-comment 'Translation definition for cycle_path_subgraph into most_root.')

(forall (x)
	(iff (atom x)
		(point x)))

(forall (x)
	(iff (bond x)
		(line x)))

(forall (x)
	(iff (functional_group x)
		(plane x)))

(forall (x)
	(iff (skeleton x)
		(space x)))

(forall (x y)
	(iff (inBond x y)
		(inpl x y)))
		
(forall (x y)
	(iff (inGroup x y)
		(inpq x y)))

(forall (x y)
	(iff (bondGroup x y)
		(inlq x y)))

(forall (x y)
	(iff (end x y)
		(planar_pendant x y)))

(forall (x y)
	(iff (star x y)
		(fork x y)))

)