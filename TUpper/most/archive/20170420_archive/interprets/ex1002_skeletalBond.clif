(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/most/interprets/ex1002_skeletalBond.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/most/definitions/most_skeleton.clif)

(cl-comment 'Things to prove')

(cl-comment 'if a bond is not a skeletal bond, then its axioms must be in tethered groups.')

(forall (b y)
	(if (not (skeletalBond b y))
		(exists (g1 g2)
			(and (not (= g1 g2))(not (= g1 y))(not (= g2 y))
				(tether g1 g2 b)))))

)