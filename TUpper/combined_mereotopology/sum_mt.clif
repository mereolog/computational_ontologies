(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/combined_mereotopology/sum_mt.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/combined_mereotopology/mt.clif)

(forall (x y)
	(exists (z)
		(forall (u)
			(iff	(C u z)
				(or	(C u x)
					(C u y))))))

)
