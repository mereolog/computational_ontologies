(cl-text  https://raw.githubusercontent.com/gruninger/colore/master/ontologies/combined_mereotopology/definitions/SC.clif

(cl-imports  https://raw.githubusercontent.com/gruninger/colore/master/ontologies/combined_mereotopology/mt.clif)

(cl-imports  https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mereology/overlaps.clif)


(forall (x)
	(iff	(SC x)
		(forall (y z)
			(if	(forall (w)
					(iff	(overlaps w x)
						(or	(overlaps w y)
							(overlaps w z))))
				(C y z)))))


)
