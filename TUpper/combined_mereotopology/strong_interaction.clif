(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/combined_mereotopology/strong_interaction.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/combined_mereotopology/mt.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/combined_mereotopology/definitions/mt_def.clif)

(forall (x y)
	(if 	(E x y)
		(part x y)))

(forall (x y)
	(if	(not (part x y))
		(exists (z)
			(and	(C z x)
				(not (C z y))))))
		
)
