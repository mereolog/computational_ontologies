(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multidim_space_physcont/definitions/enclosesmat.clif

(cl-comment 'material enclosure as specialization of detachable containment from 2013 COSIT paper')

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multidim_space_physcont/definitions/surroundsmat.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multidim_space_voids/definitions/voids_defs.clif)

(cl-comment 'MEncl-D')

(forall (x y)
	(iff
		(enclosesmat y x)
		(and
			(surroundsmat y x)
			(exists (v)
				(and
					(hostsvany y v)
					(hostscavity y v)
					(P (r x) (r y))
				)
			)
		)
	)
)


)