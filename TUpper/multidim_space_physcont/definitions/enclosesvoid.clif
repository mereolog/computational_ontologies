(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_physcont/definitions/enclosesvoid.clif

(cl-comment 'detachable containment from 2013 COSIT paper')

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_physcont/definitions/surroundsvoid.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_voids/definitions/voids_defs.clif)


(cl-comment 'MEncl-D')

(forall (x y)
	(iff
		(enclosesvoid y x)
		(and
			(surroundsvoid y x)
			(exists (v)
				(and
					(hostsvany y v)
					(hostscavity y v)
					(P (r x) (r y))
				)
			)
		)
	)
)


)