(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_physcont/definitions/isurroundsmat.clif

(cl-comment 'detachable containment from 2013 COSIT paper')

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_physcont/definitions/surroundsmat.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_voids/voids_extended_multigran.clif)

(cl-comment 'IMSur-D: incidentially surrounds a material')

(forall (x y)
	(iff
		(isurroundsmat y x)
		(and
			(surroundsmat y x)
			(not (P (r x) (voidspaceall y)))
		)
	)
)


)