(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_physcont/definitions/matfillsinside.clif

(cl-comment 'detachable containment from 2013 COSIT paper')

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_physcont/definitions/matinside.clif)

(cl-comment 'MFInside-D')

(forall (x y)
	(iff
		(matfillsinside x y)
		(and
			(matinside x y)
			(= (r x) (r y))
		)
	)
)


)