(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/occupy/no_interpenetration.clif

(cl-imports  https://raw.githubusercontent.com/gruninger/colore/master/ontologies/occupy/definitions/occupy_def.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/occupy/occupy_root.clif)

(forall (x y r1 r2)
	(if	(and	(overlaps r1 r2)
			(occupies x r1)
			(occupies y r2))
		(physical_overlaps x y)))

(forall (x y r1 r2)
        (if     (and    (occupies x r1)
                        (occupies y r2))
                (iff    (C r1 r2)
                        (physical_C x y))))

)
