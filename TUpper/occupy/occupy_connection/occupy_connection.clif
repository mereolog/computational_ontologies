(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/occupy/occupy_connection/occupy_connection.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/occupy/region_connection/region_connection.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/occupy/physical_connection/physical_connection.clif)

(forall (x)
	(if	(region x)
		(not (physical_object x))))

(forall (x y)
	(if	(occupies x y)
		(and	(physical_object x)
			(region y))))

(forall (x y z)
	(if	(and	(occupies x y)
			(occupies x z))
		(= y z)))

(forall (x)
	(if	(physical_object x)
		(exists (y)
			(occupies x y))))

(forall (x y r1 r2)
	(if	(and	(physical_C x y)
			(occupies x r1)
			(occupies y r2))
		(physical_C r1 r2)))

)
