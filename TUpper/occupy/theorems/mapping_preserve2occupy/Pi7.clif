(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/occupy/theorems/mapping_preserve2occupy/Pi7.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multigeometry/mapping_preserve.clif)

(forall (x)
	(iff	(physical_object x)
		(point x)))

(forall (x)
	(iff	(region x)
		(line x)))

(forall (x y)
	(iff	(physical_part x y)
		(part x y)))

(forall (x y)
	(iff	(spatial_physical_part x y)
		(leq x y)))

(forall (x y)
	(iff	(occupies x y)
		(and	(in x y)
			(point x)
			(line y))))

)
