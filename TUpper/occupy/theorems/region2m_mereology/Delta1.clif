(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/occupy/theorems/region2m_mereology/Delta1.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/occupy/region_mereology/region_mereology.clif)

(forall (x)
	(iff	(= x x)
		(region x)))

(forall (x)
	(iff	(part x y)
		(spatial_part x y)))

)
