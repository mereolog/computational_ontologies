(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/ordered_geometry/exog.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/ordered_geometry/wog.clif)

(forall (l)
	(if	(line l)
		(exists (x y)
			(and	(point x)
				(point y)
				(in x l)
				(in y l)
				(forall (z)
					(if	(and	(point z)
							(in z l))
						(between x z y)))))))

)
