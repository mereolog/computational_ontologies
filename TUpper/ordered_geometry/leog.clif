(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/ordered_geometry/leog.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/ordered_geometry/wog.clif)

(forall (x y z)
	(if	(between x y z)
		(exists (l)
			(and	(line l)
				(in x l)
				(in y l)
				(in z l)))))

)
