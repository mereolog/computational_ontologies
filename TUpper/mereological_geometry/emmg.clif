(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mereological_geometry/emmg.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mereological_geometry/wmg.clif)

(forall (x y l)
	(if	(and	(line l)
			(point x)
			(point y)
			(in x l)
			(in y l)
			(not (part x y)))
		(exists (z)
			(and	(point z)
				(in z l)
				(part z x)
				(disjoint_line z y l)))))

)
