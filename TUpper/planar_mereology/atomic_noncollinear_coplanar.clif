(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/planar_mereology/atomic_noncollinear_coplanar.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/planar_mereology/wpm.clif)

(forall (q l p1)
	(if	(and	(plane q)
			(in l q)
			(line l)
			(in p1 q)
			(point p1)
			(atomic_part p1))
		(exists (p2)
			(and	(point p2)
				(atomic_part p2)
				(in p2 q)
				(not (in p2 l))))))

)
