(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/planar_mereology/coplanar_mereology.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/planar_mereology/weak_planar_mereology.clif)

(forall (x q)
        (if     (and    (point x)
                        (plane q)
                        (in x q))
                (part x x)))

(forall (x y q)
        (if     (and    (plane q)
                        (in x q)
                        (in y q)
                        (part x y)
                        (part y x))
                (= x y)))

(forall (x y z q)
        (if     (and    (plane q)
                        (in x q)
                        (in y q)
                        (in z q)
                        (part x y)
                        (part y z))
                (part x z)))


)
