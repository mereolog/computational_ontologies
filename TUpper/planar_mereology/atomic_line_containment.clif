(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/planar_mereology/atomic_line_containment.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/planar_mereology/weak_planar_mereology.clif)

(forall (q l p)
	(if	(and	(plane q)
			(line l)
			(in l q)
			(atomic_part p)
			(in p l))
		(in p q)))

)
