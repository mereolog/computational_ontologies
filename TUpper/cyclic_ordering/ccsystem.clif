(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/cyclic_ordering/ccsystem.clif

(forall (x y z)
	(if	(C x y z)
		(C y z x)))

(forall (x y z)
	(if	(C x y z)
		(not (C x z y))))

(forall (x y z)
	(if	(and	(not (= x y))
			(not (= y z))
			(not (= x z)))
		(or	(C z y x)
			(C x y z))))

(forall (p q r t)
	(if	(and	(C t q r)
			(C p t r)
			(C p q t))
		(C p q r)))

(forall (p q r s t)
	(if	(and	(C t s p)
			(C t s q)
			(C t s r)
			(C t p q)
			(C t q r))
		(C t p r)))

)
