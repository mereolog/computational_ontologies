			(and (hasObservedValue a b)
				(StructuredCollection b)))))

(forall (x)
	(if (Median x)
		(StatisticalMeasure x)))

(forall (a)
	(if (MedianAbsoluteDeviation a)
		(exists (b)
			(and (hasArgument a b)
				(Median b)))))

(forall (x)
