		(Mean x)))

(forall (x)
	(if (AverageAbsoluteDeviation x)
		(StandardDispersionDeviation x)))

(forall (a)
(if (AverageAbsoluteDeviation a)
	(exists (b)
		(and
			(hasArgument a b)
			(or (Mean b)(Median b)))))) 

