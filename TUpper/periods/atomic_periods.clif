(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/periods/atomic_periods.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/periods/finite_periods.clif)

(cl-comment 'ATOM')
(forall	(x)
	(exists (y)
		(and	(inclusion y x)
			(forall	(z)
				(if	(inclusion z y)
					(= z y))))))
)	
