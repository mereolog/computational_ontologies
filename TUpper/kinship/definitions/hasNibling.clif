(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/kinship/definitions/hasNibling.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/kinship/kinship.clif)

(cl-comment 'Conservative Definitional Extension')

(cl-comment 'Axiom: HNIB-1')
(cl-comment 'The child of one persons sibling is their nibling.')
(forall (x z)
	(iff (hasNibling x z)
		(exists (y)
			(and (hasSibling x y)(hasChild y z)))))
			
)