(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/kinship/definitions/greatgrandparent.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/kinship/kinship.clif)

(cl-comment 'Conservative Definitional Extension')

(cl-comment 'Axiom: GG-1')
(cl-comment 'A great-grandparent is a parent of a parent of a parent.')
(forall (x)
	(iff (greatgrandparent x)
		(exists (w y z)
			(and (hasChild x w)(hasChild w y)(hasChild y z)
				(not (= w x))(not (= w y))(not (= w z))
				(not (= x y))(not (= y z))(not (= x z))))))

)