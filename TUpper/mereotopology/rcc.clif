(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/mereotopology/rcc.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/mereotopology/rcc_basic.clif)

(cl-comment 'RCC4a')

(forall (x y)
	(if
		(not (= y uni))
		(iff	
			(c x (compl y))
			(not (ntpp x y))
		)	
	)			
)

(cl-comment 'RCC8')

(forall (x)
	(exists (y)
		(ntpp y x)
	)
)


)
