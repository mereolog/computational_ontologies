(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/mereotopology/rt.clif

(cl-comment 'axioms of the mereotopology RT')

(cl-imports  https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/mereotopology/rt_minus.clif)

(cl-comment 'D11: Weak contact')

(forall (x y)
	(iff	(wcont x y)
		(and 	(not (c (cl x) (cl y)))
			(forall (z)
				(if 	
					(and	
						(p x z)
						(op z)
					)
					(c (cl z) y)
)	)	)	)	)


(cl-comment 'A12: Existence of a weak contact')

(exists (x y)
	(wcont x y)
)

)
