(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/cayley_graph/cayley_root.clif

(forall (x y z)
	(if	(apply x y z)
		(generator x)))

(forall (x)
	(if	(generator x)
		(forall (y)
			(exists (z)
				(apply x y z)))))

(forall (x y z w)
	(if	(and	(apply x y z)
			(apply x y w))
		(= z w)))

(forall (x y z w)
	(if	(and	(apply x y z)
			(apply w y z))
		(= x w)))

(forall (x y z w)
	(if	(and	(apply y x z)
			(apply y w z))
		(= x w)))

(forall (x y)
	(not (apply x y y)))

(forall (x y z w)
	(if	(and	(apply x y z)
			(generator z)
			(generator w))
		(apply w y w)))

)
