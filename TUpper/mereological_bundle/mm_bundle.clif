(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/mereological_bundle/mm_bundle.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/mereological_bundle/m_bundle.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/mereological_bundle/mereological_bundle_def.clif)

(forall (x y p)
        (if     (tppart x y p)
                (exists  (z)
                        (and    (tppart z y p)
                                (tdisjoint z x p)))))


)
