formulas(assumptions).

%original interval meeting axioms

(all i all j all k all m
	((meets(i,k)
	& meets(j,k)
	& meets(i,m))
	->
	meets(j,m))).

(all i all j all k all l
	((meets(i,j)
	& meets(k,l))
	->
	(meets(i,l)
	| (exists n
		((meets(i,n) & meets(n,l))
		| (meets(k,n) & meets(n,j))))))).

(all i all j
	(meets(i,j)
	->
	-meets(j,i))).

(all i all j all k all m
	((meets(i,j)
	& meets(j,k)
	& meets(k,m))
	->
	(exists n
		(meets(i,n)
		& meets(n,m))))).

%definitions

all x all y (m(x,y) <-> meets(x,y)).

(all x all y
	(b(x,y)
	<->
	(exists z
		(meets(x,z)
		& meets(z,y))))).

(all i all j
	(d(i,j)
	<->
	(exists k exists m exists n exists o
		(meets(k,m)
		& meets(m,i)
		& meets(i,n)
		& meets(n,o)
		& meets(k,j)
		& meets(j,o))))).

end_of_list.

formulas(goals).

all x all y all z ((d(x,y) & m(y,z)) -> b(x,z)).

end_of_list.

