(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/owltime_instants/mappings/owltime2orderings.clif

(forall (x y)
	(iff	(lt x y)
		(and	(Instant x)
			(Instant y)
			(before x y))))

(forall (x y)
	(iff	(leq x y)
		(and	(Instant x)
			(Instant y)
			(or	(before x y)
				(= x y)))))

)
