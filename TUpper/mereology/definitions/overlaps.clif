(cl-text  https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mereology/definitions/overlaps.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mereology/m_mereology.clif)

(forall (x y)
(iff    (overlaps x y)
        (exists (z)
                (and    (part z x)
                        (part z y)))))

)
