(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mereology/ppp_mm_mereology.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mereology/mm_mereology.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mereology/definitions/ppart.clif)

(cl-comment 'Proper Parts Principle')
(forall (x y w)
        (if     (and	(ppart w x)
		 	(not (part x y)))
		(exists (z)
			(and	(ppart z x)
				(not (ppart z y))))))

)
