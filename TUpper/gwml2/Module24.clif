(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/module24.clif
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/definitions/module13.clif)
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/module23.clif)


(cl-comment 'WBS-A1')
(cl-comment 'Water Body Surface is a specialization of NAPO')
(forall (x)
	(if
		(WBS x)
		(and
			(FBS x)
			(exists (y)
				(and
					(hosts y x)
					(WB y)
				)
			)
		)
	)
)
)