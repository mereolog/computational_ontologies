(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/gwml2/module32.clif
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/gwml2/module1.clif)


(cl-comment 'MX-A1')
(cl-comment 'Mixture is a specialization of Fluid Matter')
(forall (x)
	(if
		(MX x)
		(FM x)
	)
)
(cl-comment 'MX-A2')
(cl-comment 'Mixture is an aggregate variety of Fluid Matter and some other solid Matter or Fluid Matter that it either intergranularly contains or fully physically contains')
(forall (x)
	(if
		(MX x)
		(exists (y z)
			(and
				(FM y)
				(EM z)
				(not (= x y))
				(not (= z y))
				(not (= z x))
				(intragranularconstituent y x)
				(fullyphysicallycontains y z)
				(fullyphysicallycontains x z)
			)
		)
	)
)
)