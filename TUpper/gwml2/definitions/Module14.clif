(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2/definitions/module14.clif
(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2/definitions/module13.clif)
(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2/definitions/module11.clif)
(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2/module9.clif)


(cl-comment 'WWB-D')
(forall (x)
	(iff
		(WWB x)
		(and
			(WB x)
			(exists (y v)
				(and
					(W y)
					(forall (z)
						(if
							(and
								(intragranularconstituent z y)
								(WM z)
							)
							(submaterial z x)
						)
					)
					(HGV v)
					(P (r x) (r v))
				)
			)
		)
	)
)
)