(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/gwml2/module27.clif
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/gwml2/module1.clif)
(cl-comment 'NEED TO IMPORT AXIOMATIZATION FOR CONSTITUTIENTS AND GRAIN COMPARISON')


(cl-comment 'Const-D')
(forall (x)
	(iff
		(Const x)
		(and
			(M x)
			(exists (y)
				(and
					(FM y)
					(fullyphyscontains y x)
					(lessergrain x y)
				)
			)
		)
	)
)
)