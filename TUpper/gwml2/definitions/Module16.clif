(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/definitions/module16.clif
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/definitions/module15.clif)

(cl-comment 'SSWB-D')
(forall (x)
	(iff
		(SSWB x)
		(and
			(WB x)
			(forall (y w)
				(if
					(and
						(submaterial y x)
						(WWB w)
						(not (PO (r y) (r w)))
					)
					(exists (z)
						(and
							(HGU z)
							(submaterial x z)
						)
					)
					
				)
			)
		)
	)
)
)