(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2/definitions/module19.clif
(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2/definitions/module10.clif)

(cl-comment 'CSB-D')
(forall (x)
	(iff
		(CSB x)
		(and
			(SB x)
			(exists (y)
				(and
					(HV y)
					(hostsv x y)
				)
			)
		)
	)
)
)