(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/gwml2/definitions/module11.clif
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/gwml2/definitions/module10.clif)
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/gwml2/module9.clif)
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/gwml2/module5.clif)

(cl-comment 'HGV-D')
(forall (x)
	(iff
		(HGV x)
		(and
			(HV x)
			(exists (y z)
				(and
					(SB y)
					(hostsv y x)
					(submaterial y z)
					(or
						(HGU z)
						(W z)
					)
				)
			)
		)
	)
)
)