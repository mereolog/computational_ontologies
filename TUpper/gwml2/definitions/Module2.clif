(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/definitions/module2.clif
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/module1.clif)

(cl-comment 'NEED TO IMPORT AXIOMATIZATION FOR  CONSTITUTIONS')

(cl-comment 'SB-D')
(cl-comment 'Solid Body is a NAPO that is constituted by Matter but not Fluid Matter')
(forall (x)
	(iff
		(SB x)
		(and
			(NAPO x)
			(exists (y)
				(and
					(M y)
					(intragranularconstituent y x)
				)	
			)
			(forall (z)
				(if 
					(intragranularconstituent z x)
					(not (FM z))
				)
			)
		)
	)
)

)