(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2/definitions/module21.clif
(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2/definitions/module20.clif)

(cl-comment 'CB-D')
(forall (x)
	(iff
		(CB x)
		(and
			(AU x)
			(not (exists (y)
					(and
						(WB y)
						(submaterial y x)
					)
				)	
			)
		)
	)
)

)