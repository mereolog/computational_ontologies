(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/module12.clif
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/module1.clif)

(cl-comment 'NEED TO IMPORT AXIOMATIZATION FOR  CONSTITUTIONS')


(cl-comment 'FB-A1')
(cl-comment 'Fluid Body is a specialization of NAPO')
(forall (x)
	(if
		(FB x)
		(NAPO x)
	)
)
(cl-comment 'FB-A2')
(cl-comment 'Any intragranular constituent of a FLuid Body is FLuid Matter')
(forall (x y)
	(if
		(and
			(FB x)
			(intragranularconstituent y x)
		)
		(FM y)
	)
)
)