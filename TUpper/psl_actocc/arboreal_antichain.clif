(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_actocc/arboreal_antichain.clif

(forall (s1 s2)
	(if	(arboreal_subocc s1 s2)
		(and	(arboreal s1)
			(arboreal s2))))

(forall (s)
	(if	(arboreal s)
		(arboreal_subocc s s)))

(forall (s1 s2)
	(if	(and	(arboreal_subocc s1)
			(arboreal_subocc s2))
		(= s1 s2)))

(forall (s1 s2 s3)
	(if	(and	(arboreal_subocc s1 s2)
			(arboreal_subocc s2 s3))
		(arboreal_subocc s1 s3)))

(forall (s1 s2)
	(if	(arboreal_subocc s1 s2)
		(= s1 s2)))

)
