(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_actocc/arboreal_subocc.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_actocc/mereo_subocc.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_actocc/arboreal_antichain.clif)

(forall (o)
	(if	(arboreal o)
		(activity_occurrence o)))

(forall (s1 s2)
	(if	(arboreal_subocc s1 s2)
		(subactivity_occurrence s1 s2)))

(forall (o)
	(iff	(arboreal o)
		(atom_subocc o)))

(forall (o1)
	(iff	(atom_subocc o1)
		(and	(activity_occurrence o1)
			(forall (o2)
				(if	(subactivity_occurrence o2 o1)
					(= o1 o2))))))
	

)
