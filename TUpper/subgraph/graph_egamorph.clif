(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/subgraph/graph_egamorph.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/subgraph/mapping_graph.clif)

(forall (p1 p2 l1 l2)
	(if	(and	(point p1)
			(point p2)
			(line l1)
			(line l2)
			(in p1 l1)
			(in p2 l2)
			(adj p1 p2)
			(not (= (l1 l2))))
		(adj l1 l2)))

)
