(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/bipartite_incidence/interior_trivial.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/bipartite_incidence/weak_bipartite.clif)

(forall (x y l1)
	(if	(and	(point x)
			(point y)
			(not (= x y))
			(line l1)
			(in x l1)
			(in y l1))
		(exists (l2)
			(and	(line l2)
				(not (= l1 l2))
				(or	(in x l2)
					(in y l2))))))

)
