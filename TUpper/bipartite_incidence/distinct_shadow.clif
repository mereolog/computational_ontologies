(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/distinct_shadow.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/weak_bipartite.clif)

(forall (x y)
	(if	(and	(line x)
			(line y)
			(not (= x y)))
		(exists (z)
			(and	(point z)
				(in z x)
				(not (in z y))))))
		

)
