(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/bipartite_incidence/point_bipartite.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/bipartite_incidence/weak_bipartite.clif)

(forall (p)
	(if	(point p)
		(exists (l)
			(and	(line l)
				(in p l)))))

)
