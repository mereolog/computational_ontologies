(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/weak_simple_leafless.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/symmetric_graphical.clif)

(forall (l1 p1 p2 p4)
        (if     (and    (line l1)
			(point p1)
			(point p2)
			(point p4)
			(in p1 l1)
			(in p2 l1)
			(in p1 l2)
			(in p4 l2)
                        (not (= p2 p4)))
                (exists (p3 l3)
                        (and    (line l3)
				(point p3)
				(in p2 l3)
				(in p3 l3)
                                (not (= p3 p1))))))



)
