(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/double_pendant_points.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/weak_bipartite.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/definitions/bipartite_def.clif)

(forall (x y z)
	(if	(and	(pendant_point x)
			(pendant_point y)
			(pendant_point z))
		(or	(= x y)
			(= x z)
			(= y z))))

)
