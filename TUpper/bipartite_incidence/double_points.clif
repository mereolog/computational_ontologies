(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/bipartite_incidence/double_points.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/bipartite_incidence/weak_bipartite.clif)

(cl-comment 'A geometry in which at most two points can be incident with a line')

(forall (x y z l)
	(if	(and	(point x)
			(point y)
			(point z)
			(line l)
			(in x l)
			(in y l)
			(in z l))
		(or	(= z x)
			(= z y)
			(= x y))))

)
