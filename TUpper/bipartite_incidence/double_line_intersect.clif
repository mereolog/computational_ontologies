(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/bipartite_incidence/double_line_intersect.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/bipartite_incidence/weak_bipartite.clif)

(forall (l1 l2 l3 p1 p2 p3 p4 p5 p6 p7)
	(if	(and	(line l1)
			(line l2)
			(line l3)
			(point p1)
			(in p1 l1)
			(in p1 l2)
			(in p1 l3)
			(point p2)
			(point p3)
			(not (= p2 p3))
			(in p2 l1)
			(in p3 l1)
			(point p4)
			(point p5)
			(not (= p4 p5))
			(in p4 l2)
			(in p5 l2)
			(point p6)
			(point p7)
			(not (= p6 p7))
			(in p6 l3)
			(in p7 l3))
		(or	(= l1 l2)
			(= l1 l3)
			(= l2 l3))))

)
