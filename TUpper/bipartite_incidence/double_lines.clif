(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/double_lines.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/weak_bipartite.clif)

(forall (x y z w)
	(if	(and	(point x)
			(line y)
			(line z)
			(line w)
			(in x y)
			(in x z)
			(in x w))
		(or	(= w z)
			(= y z)
			(= w y))))

)
