(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_core/mandatory.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_core/psl_participates.clif)

(cl-comment 'Every object has to participate in some time object.')
(forall (x)
	(if	(object x)
		(exists (o t)
			(participates_in x o t))))

(forall (o t)
	(if	(and	(activity_occurrence o)
			(is_occurring_at o t))
		(exists (x)
			(participates_in x o t))))

)
