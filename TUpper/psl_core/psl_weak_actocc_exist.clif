(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_core/psl_weak_actocc_exist.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_core/psl_participates.clif)

(forall (o t)
	(if	(and	(activity_occurrence o)
			(is_occurring_at o t))
		(exists (x)
			(participates_in x o t))))

)
