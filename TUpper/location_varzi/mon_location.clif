(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/location_varzi/mon_location.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/location_varzi/location_root.clif)

(forall (x y z w)
	(if	(and	(part x y)
			(L x z)
			(L y w))
		(part z w)))

(forall (x y)
	(if	(and	(L x y)
			(part z y))
		(exists (w)
			(and	(part w x)
				(L w z)))))

)
