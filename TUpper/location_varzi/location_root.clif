(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/location_varzi/location_root.clif


(forall (x y z)
	(if	(and	(L x y)
			(L x z))
		(= y z)))

(forall (x y)
	(if	(L x y)
		(L y y)))

)

(forall (x)
	(iff	(R x) (L x x))

)

