(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/location_varzi/mt_location.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/location_varzi/location_root.clif)

(forall (x y z w)
	(if	(and	(part x y)
			(L x z)
			(L y w))
		(part z w)))

(forall (x y z w)
	(if	(and	(C x y)
			(L x z)
			(L y w))
		(C z w)))

)
