formulas(assumptions).

(all x all y
	(leq(x,y)
	->
	(D(x) & D(y)))).

% Reflexive
all x
	(D(x) -> leq(x,x)).

% Antisymmetry
all x all y
	((leq(x,y)
	& leq(y,x))
	->
	(x=y)).

% Transitive
all x all y all z
	((leq(x,y)
	& leq(y,z))
	->
	leq(x,z)).

(all x all y
	(lt(x,y)
	<->
	(leq(x,y)
	& (x != y)))).

%def1

all x 
    (D(x) <-> generator(x)).

all x all y
    (leq(x,y) <-> generator_subactivity(x,y)).

end_of_list.

formulas(goals).

all a 
    (generator(a)
    -> generator_subactivity(a, a)).

end_of_list.

