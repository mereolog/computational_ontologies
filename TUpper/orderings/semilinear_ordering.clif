(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/orderings/semilinear_ordering.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/orderings/partial_ordering.clif)

(forall (x y z)
	(if	(and	(leq x y)
			(leq z y))
		(or	(leq x z)
			(leq z x))))

(forall (x y)
	(exists (z)
		(and	(leq z x)
			(leq z y))))

)
