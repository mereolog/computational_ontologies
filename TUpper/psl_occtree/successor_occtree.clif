(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_occtree/successor_occtree.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_occtree/earlier_occtree.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_occtree/succ_structure.clif)

(forall (o1 o2)
	(if	(earlier o1 o2)
		(exists (o3 o4)
			(succ o4 o3 o2))))

(forall (s1 s2 s3 s4)
	(if	(succ s2 s3 s4)
		(iff	(earlier s1 s4)
			(earlierEq s1 s3))))

)
