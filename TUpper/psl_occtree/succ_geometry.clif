(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_occtree/succ_geometry.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_occtree/genocc.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_occtree/succ_structure.clif)

(forall (o1 o2 o3)
	(if	(succ o1 o2 o3)
		(and	(arboreal o1)
			(arboreal o2)
			(arboreal o3))))

(forall (o1 o2 o3)
	(if	(succ o1 o2 o3)
		(forall (a)
			(iff	(occurrence_of o1 a)
				(occurrence_of o2 a)))))

(forall (a)
	(if	(generator a)
		(exists (s)
			(and	(initial s)
				(occurrence_of s a)))))

(forall (s1 s2 a)
	(if	(and	(initial s1)
			(initial s2)
			(occurrence_of s1 a)
			(occurrence_of s2 a))
		(= s1 s2)))


)
