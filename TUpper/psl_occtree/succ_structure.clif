(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_occtree/succ_structure.clif


(forall (s1 s2 s3)
	(if	(succ s1 s2 s3)
		(initial s1)))

(forall (s1)
	(if	(initial s1)
		(forall (s2)
			(exists (s3)
				(succ s1 s2 s3)))))

(forall (s1 s2 s3 s4)
	(if	(and	(succ s1 s2 s3)
			(succ s1 s2 s4))
		(= s3 s4)))

(forall (s1 s2 s3 s4)
	(if	(and	(succ s1 s2 s3)
			(succ s4 s2 s3))
		(= s1 s4)))

(forall (s1 s2 s3 s4)
	(if	(and	(succ s2 s1 s3)
			(succ s2 s4 s3))
		(= s1 s4)))

(forall (s1 s2)
	(not (succ s1 s2 s2)))

(forall (s1 s2 s3 s4 s5)
	(and	(succ s1 s2 s3)
		(succ s4 s5 s3))
	(= s1 s4))

(forall (s1)
	(iff	(initial s1)
		(not (exists (s2 s3)
			(succ s2 s3 s1)))))
	
)
