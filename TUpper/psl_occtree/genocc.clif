(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/psl_occtree/genocc.clif

(forall (x)
	(if	(atomic x)
		(not (arboreal x))))

(forall (a o)
	(if	(occurrence_of o a)
		(iff	(generator a)
			(arboreal o))))

(forall (a)
	(iff	(generator a)
		(and	(atomic a)
			(exists (o)
				(and	(arboreal o)
					(occurrence_of o a))))))

(forall (a)
	(if	(atomic a)
		(activity a)))

(forall (o)
	(if	(arboreal o)
		(activity_occurrence o)))

)
