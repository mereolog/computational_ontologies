(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_occtree/occtree_time.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_occtree/earlier_occtree.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_core/occ_time.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/psl_core/psl_before.clif)

(cl-comment 'The endof an activity occurrence is before to the beginof
the successor of the activity occurrence.')

(forall (s1 s2)
        (if     (earlier s1 s2)
                (before (endof s1) (beginof s2))))


)
