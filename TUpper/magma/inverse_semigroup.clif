(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/magma/inverse_semigroup.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/magma/semigroup.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/magma/definitions/magma_defs.clif)

(forall (x)
	(exists (y)
		(and 	(= y (op y (op x y)))
			(= x (op x (op y x)))))) 

(forall (x y z)
        (if     (and    (inverse x y)
                        (inverse x z))
                (= y z)))


)

