(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/quality/direct_quality.clif

(forall (x y1 y2)
	(if (and (dqt x y1)(dqt x y2))
		(= y1 y2)))

(forall (x1 x2 y)
	(if (and (dqt x1 y)(dqt x2 y)(TQ x1)(TQ x2)(PD y))
		(= x1 x2)))

(forall (x)
	(if (TQ x)
		(exists (y) (and (PD y)(dqt x y)))))
		
)

