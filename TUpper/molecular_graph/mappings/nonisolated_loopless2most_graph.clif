(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/molecular_graph/mappings/most_graph_reduction.clif

(cl-comment 'Translation definition for nonisolated_loopless into most_graph.')

(forall (x)
	(iff (atom x)
		(point x)))

(forall (x)
	(iff (bond x)
		(line x)))

(forall (x y)
	(iff (mol x y)
		(in x y)))
		
)