(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/poset_foliation/downward_p_foliation.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/poset_foliation/p_foliation.clif)

(forall (x y p1 p2)
	(if	(and	(tleq x y p1)
			(part p2 p1))
		(tleq x y p2)))

)
