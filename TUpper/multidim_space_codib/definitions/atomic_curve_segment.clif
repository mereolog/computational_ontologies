(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codib/definitions/atomic_curve_segment.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codib/definitions/simple_curve_segment.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/atomic_s.clif)


(cl-comment 'An atomic curve segment is a curve that contains exactly two distinct endpoints and is simple, i.e., is not branched.')

(forall (x)
	(iff
		(AtomicCurveSegment x)
		(and
			(SimpleCurveSegment x)
			(Atomic_S x)
		)
	)
)

)
