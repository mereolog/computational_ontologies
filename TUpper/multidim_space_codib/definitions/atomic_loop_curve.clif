(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codib/definitions/atomic_loop_curve.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codib/definitions/simple_loop_curve.clif)


(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codib/codib.clif)


(cl-comment 'An atomic loop curve is a closed (unbounded) curve without proper parts that contains exactly one point and that contains no endpoint.')

(forall (x)
	(iff
		(AtomicLoopCurve x)
		(and
			(SimpleLoopCurve x)
			(Atomic_S x)
		)
	)
)

)
