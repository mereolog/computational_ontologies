(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/density/definitions/physical_density.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/density/density.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/mass/mass.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/size/volume.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/occupy/occupy_root.clif)

(forall (x m r)
	(iff	(physical_density x (dmv (mass m) (volume r)))
		(and	(constitutes m x)
			(occupies m r)
			(VoluminalRegion r))))
)
