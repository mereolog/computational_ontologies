(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/lattices/ortho_lattice.clif

(cl-comment 'Orthocomplemented lattice axioms according to Beran 1976')

(cl-comment 'O2.1')

(forall (x y z)
	(= 	(join (join x y) z)
		(join (meet (oc z) (oc y)) x))
)

(cl-comment 'O2.2')

(forall (x y)
	(= x (meet x (join x y)))
)

(cl-comment 'O2.3')

(forall (x y)
	(= x (join x (meet y (oc y))))
)


)
