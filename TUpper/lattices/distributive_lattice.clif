(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/lattices/distributive_lattice.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/lattices/lattice.clif)

(cl-comment 'Distributive lattice axioms')

(cl-comment 'L8: Distributivity')

(forall (x y z)
	(= (meet (x (join y z)))
		(join (meet x y) (meet x z))))

)
