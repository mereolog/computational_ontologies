(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/lattices/stone_lattice.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/lattices/pseudo_complemented_distributive_lattice.clif)

(cl-comment 'Defining the Stone lattices as pseudocomplemented distributive lattices satisfying the Stone identity')

(cl-comment 'S1: Stone identity')

(forall (x y)
	(= (pc (meet x y)) (join (pc x) (pc y)))
)


)
