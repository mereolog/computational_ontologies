(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/lattices/definitions/complement.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/lattices/bounded_lattice.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/lattices/definitions/meetcomplement.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/lattices/definitions/joincomplement.clif)


(cl-comment 'D6: Definition of a complement (binary relation, not unique) as join- and meet-complement')

(forall (x y)
	(iff 
		(complement x y)
		(and
			(meetcomplement x y)
			(joincomplement x y))))

)
