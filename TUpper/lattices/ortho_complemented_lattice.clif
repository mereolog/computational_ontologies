(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/lattices/ortho_complemented_lattice.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/lattices/complemented_lattice.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/lattices/definitions/lattice_def.clif)

(cl-comment 'Orthocomplemented lattice axioms')

(cl-comment 'OC1: Orthocomplement is complement')

(forall (x)
	(complement x (oc x)))

(cl-comment 'OC2: Orthocomplement is involutary')

(forall (x)
	(= x (oc (oc x)))
)

(cl-comment 'OC3: Orthocomplement is order-reversing')


(forall (x y)
(iff
	(leq x y)
	(leq (oc y) (oc x))	
))

)
