(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/owltime_interval/owltime_leu.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/owltime_interval/owltime_le.clif)

(forall (t1 t2 i1 i2)
	(if	(and	(Interval i1)
			(Interval i2)
			(begins t1 i1)
			(ends t2 i1)
			(begins t1 i2)
			(ends t2 i2))
		(= i1 i2)))

)
