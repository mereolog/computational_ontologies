(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/subposet/comparable_preserve.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/subposet/partial_subposet.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/orderings/definitions/comparable.clif)

(forall (x y z)
	(if	(and	(part x z)
			(comparable x y)
			(not (= x y)))
		(comparable y z)))

)
