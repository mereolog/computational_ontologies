(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/subposet/subposet_root.clif

(cl-module mereo_element (cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/mereology/m_mereology.clif))

(cl-module poset_element (cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/orderings/quasiorder.clif))

(forall (x y)
	(if	(part x y)
		(and	(mereo_element x)
			(mereo_element y))))

(forall (x y)
	(if	(leq x y)
		(and	(poset_element x)
			(poset_element y))))

)
