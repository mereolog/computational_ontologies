(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/subposet/chain_antichain.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/subposet/partial_subposet.clif)

(forall (x y)
	(if	(or	(part x y)
			(part y x))
		(not 	(or	(lt x y)
				(lt y x)))))

)
