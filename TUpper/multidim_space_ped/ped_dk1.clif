(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_ped/ped_dk1.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_ped/ped.clif)


(cl-comment 'PED-A9:')

(forall (x y)
	(if
		(DK1 x y)
		(M x)
	)
)


(cl-comment 'PED-A9:')

(forall (x y)
	(if
		(DK1 x y)
		(or
			(POB y)
			(RPF y)
		)
	)
)

(cl-comment 'PED-A10:')

(forall (x y z)
	(if
		(and
			(DK1 x y)
			(DK1 z y)
		)
		(= x z)
	)
)
		

(cl-comment 'PED-A11:')

(forall (y)
	(if
		(or
			(POB y)
			(RPF y)
		)
		(exists (x)
			(DK1 x y)
		)
	)
)

)