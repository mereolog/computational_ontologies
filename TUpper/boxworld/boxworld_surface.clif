(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/boxworld/boxworld_surface.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/boxworld/boxworld_edge.clif)

(cl-comment 'Each point, edge, and surface is part of a unique box.')
(forall (x1 x2 y)
        (if     (and    (part y x1) (part y x2) (box x1) (box x2)
                        (or (point y) (edge y) (surface y)))
                (= x1 x2)))
)

