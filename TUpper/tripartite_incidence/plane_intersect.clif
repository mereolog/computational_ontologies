(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/tripartite_incidence/plane_intersect.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/tripartite_incidence/weak_tripartite.clif)

(forall (p q)
	(if	(and	(plane q)
			(point p)
			(in p q))
		(exists (l1 l2)
			(and	(line l1)
				(line l2)
				(not (= l1 l2))
				(in p l1)
				(in p l2)
				(in l1 q)
				(in l2 q)))))

)
