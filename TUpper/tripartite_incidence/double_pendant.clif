(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/double_pendant.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/weak_tripartite.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/definitions/tripartite_def.clif)

(forall (q x y z)
	(if	(and	(planar_pendant x q)
			(planar_pendant y q)
			(planar_pendant z q))
		(or	(= x y)
			(= y z)
			(= z x))))

)
