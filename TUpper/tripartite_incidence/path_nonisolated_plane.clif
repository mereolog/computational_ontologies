(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/tripartite_incidence/path_nonisolated_plane.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/tripartite_incidence/weak_tripartite.clif)

(forall (q)
	(if	(plane q)
		(exists (p l q2)
			(and	(line l)
				(in l q)
				(point p)
				(in p l)
				(plane q2)
				(in p q2)))))

)
