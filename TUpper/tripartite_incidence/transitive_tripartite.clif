(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/transitive_tripartite.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/weak_tripartite.clif)

(forall (x y z)
	(if	(and	(plane x)
			(line y)
			(point z)
			(in z y)
			(in y x))
		(in z x)))

)
