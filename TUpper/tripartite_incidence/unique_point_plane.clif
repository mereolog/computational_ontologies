(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/tripartite_incidence/unique_point_plane.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/tripartite_incidence/weak_tripartite.clif)

(forall (p q1 q2)
	(if	(and	(point p)
			(plane q1)
			(plane q2)
			(in p q1)
			(in p q2))
		(= q1 q2)))

)
