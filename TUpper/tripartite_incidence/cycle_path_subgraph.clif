(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/cycle_path_subgraph.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/induced_subgraph.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/bipartite_incidence/weak_loopless.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/coplanar_double_collinear.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/double_pendant.clif)

(forall (p q)
	(iff (star p q)
		(exists (l1 l2 l3)
			(and (line l1)(line l2)(line l3)
				(not (= l1 l2))
				(not (= l1 l3))
				(not (= l2 l3))
				(in p l1)
				(in p l2)
				(in p l3)
				(in l1 q)
				(in l2 q)
				(in l3 q)))))

)
