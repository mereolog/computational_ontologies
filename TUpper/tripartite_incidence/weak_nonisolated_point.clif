(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/weak_nonisolated_point.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/weak_tripartite.clif)

(forall (p l)
	(if	(and	(point p)
			(in p l)
			(line l))
		(exists (q l2)
			(and	(plane q)
				(line l2)
				(in p q)
				(in l2 q)))))

)
