(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/coplanar_line_existence.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/tripartite_incidence/weak_tripartite.clif)

(forall (p1 p2 q)
	(if	(and	(point p1)
			(point p2)
			(plane q)
			(in p1 q)
			(in p2 q)
			(not (= p1 p2)))
		(exists (l)
			(and	(line l)
				(in p1 l)
				(in p2 l)
				(in l q)))))


)
