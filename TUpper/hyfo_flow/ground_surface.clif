(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/hydrologicFlow/ground_surface.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/gwml2-v2/geo_geologicunit.clif)
(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_ped/ped.clif)


(cl-comment 'Z-A1: Ground surface is a relevant part feature hosted by a geologic unit')
(forall (x)
	(if
		(GS x)
		(and
			(RPF x)
			(exists (y)
				(and
					(GU y)
					(hosts y x)
				)
			)
		)
	)
)

)

