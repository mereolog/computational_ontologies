(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multigeometry/collinear_antichain.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multigeometry/multi_g.clif)

(forall (p1 p2 p3 l)
	(if	(and	(point p1)
			(point p2)
			(not (= p1 p2))
			(line l)
			(point p3)
			(part p3 p1))
		(not (part p3 p2))))

)
