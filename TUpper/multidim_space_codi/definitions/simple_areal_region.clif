(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/simple_areal_region.clif


(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/areal_region.clif)


(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/simple_s.clif)


(cl-comment 'A simple areal region is an areal region that is non-branching and thus planar and also connected.')


(forall (x)
	(iff
		(Simple_ArealRegion x)
		(and
			(ArealRegion x)
			(Simple_S x)
		)
	)
)

)
								