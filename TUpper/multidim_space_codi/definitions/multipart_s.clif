(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_codi/definitions/multipart_s.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_codi/definitions/connected_s.clif)

(cl-comment 'Defining when an entity is a multipart spatial entity')

(cl-comment 'MS-D')

(forall (x)
	(iff
		(Multipart_S x)
        (and
            (S x)
			(not (Connected_S x))
        )
	)
)



)