(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multidim_space_codi/definitions/voluminal_region.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multidim_space_codi/definitions/areal_region.clif)

(cl-comment 'Definition of an voluminal_region as something that is of exactly one dimension greater than an areal_region')


(forall (x)
	(if
		(VoluminalRegion x)
		(forall (y)
			(iff 
				(ArealRegion y)
				(Covers x y)
			)
		)
	)
)


(forall (x y)
	(if
		(and
			(ArealRegion y)
			(Covers x y)
		)
		(VoluminalRegion x)
	)
)

)