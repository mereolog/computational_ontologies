(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/simple_curve.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/curve.clif)

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multidim_space_codi/definitions/simple_s.clif)


(cl-comment 'A simple curve is a curve that is simple.')


(forall (x)
	(iff
		(SimpleCurve x)
		(and
			(Curve x)
			(Simple_S x)
		)
	)
)


)
