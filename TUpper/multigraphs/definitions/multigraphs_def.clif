(cl-text https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multigraphs/definitions/multigraphs_defs.clif

(cl-imports https://raw.githubusercontent.com/gruninger/colore/master/ontologies/multigraphs/pseudograph.clif)

(forall (x)
	(iff	(path_endpoint x)
		(and	(vertex x)
			(forall (e1 e2 y z)
				(if	(and	(madj e1 x y)
						(madj e2 x z))
					(= y z))))))

)
