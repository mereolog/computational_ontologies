(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multigraphs/double_endpoint.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multigraphs/pseudograph.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multigraphs/definitions/multigraphs_def.clif)

(forall (x y z)
	(if	(and	(path_endpoint x)
			(path_endpoint y)
			(path_endpoint z))
		(or	(= x y)
			(= x z)
			(= y z))))
	
)
