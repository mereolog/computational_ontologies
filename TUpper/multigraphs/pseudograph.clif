(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/tupper/multigraphs/pseudograph.clif

(forall (e x y)
	(if	(madj e x y)
		(and	(edge e)
			(vertex x)
			(vertex y))))

(forall (x)
	(if	(edge x)
		(not (vertex x))))
		
(forall (x)
	(if	(edge x)
		(exists (y z)
			(madj x y z))))

(forall (e x y z u v)
	(if	(and	(madj e x y)
			(madj e u v))
		(or	(= x u)
			(= x v))))

(forall (e x y z u v)
	(if	(and	(madj e x y)
			(madj e u v))
		(or	(= y u)
			(= y v))))

(forall (e x y)
	(if	(madj e x y)
		(madj e y x)))

(forall (x)
	(or	(vertex x)
		(edge x)))

)
