(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/combined_time/no_backwards.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/combined_time/sim_vc_end.clif)

(forall (i1)
	(if	(timeinterval i1)
		(not (exists (i2)
			(and	(timeinterval i2)
				(= (beginof i2) (endof i1))
				(= (endof i2) (beginof i1)))))))


)
