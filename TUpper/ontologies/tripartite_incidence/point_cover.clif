(cl-text http://colore.oor.net/tripartite_incidence/point_cover.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (q p l)
	(if	(and	(plane q)
			(point p)
			(line l)
			(in p q)
			(in l q))
		(in p l)))

)
