(cl-text http://colore.oor.net/orderings/definitions/lub.clif

(cl-imports http://colore.oor.net/orderings/quasiorder.clif)

(forall (x y s) 
	(iff	
        (lub x y s)
		(and 	
            (leq x s)
			(leq y s)
			(forall (z)
				(if	(and	
                        (leq x z)
						(leq y z)
                    )
					(leq s z))))))


)
