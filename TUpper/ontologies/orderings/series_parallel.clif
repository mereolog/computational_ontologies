(cl-text http://colore.oor.net/orderings/series_parallel.clif

(cl-imports http://colore.oor.net/orderings/partial_ordering.clif)

(cl-imports http://colore.oor.net/orderings/definitions/incomparable.clif)

(forall (x y z w)
        (if     (and	(leq x y)
			(leq z y)
			(leq z w))
		(or	(not (incomparable x w))
			(not (incomparable y w))
			(not (incomparable x z)))))

)
