(cl-comment 'Translation definitions for most_group into cycle_path_subgraph_nonisolated.')

(forall (x)
	(iff (point x)
		(atom x)))

(forall (x)
	(iff (line x)
		(bond x)))

(forall (x)
	(iff (plane x)
		(group x)))
		
(forall (x y)
	(iff (end x y)
		(planar_pendant x y)))

(forall (x y)
	(iff (in x y)
		(mol x y)))

