(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/definitions/module20.clif
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/definitions/module15.clif)
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gwml2/definitions/module19.clif)


(cl-comment 'A-D')
(forall (x)
	(iff
		(A x)
		(and
			(AU x)
			(not (exists (y)
				(and
					(AU y)
					(not (= x y))
					(submaterial y x)
				)
			))
			(exists (s a)
				(and
					(CSB s)
					(submaterial s x)
					(AWB a)
					(submaterial a x)
					(not (ZEX (convoidspace s)))
					(forall (w)
						(if 
							(intragranularconstituent w a)
							(P (r w) (convoidspace s))
						)
					)
				)
			)
		)
	)
)

)