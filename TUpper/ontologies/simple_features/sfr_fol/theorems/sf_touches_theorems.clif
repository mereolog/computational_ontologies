(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/simple_features/sfr_fol/theorems/sf_touches_theorems.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/simple_features/sfr_fol/sf_touches.clif)


(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/simple_features/sfc_core/sf_point.clif)

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_codi/definitions/sc.clif)


(cl-comment 'SFR-T1')


(forall (x y)
	(if	
		(sf_touches x y)
		(SC x y)
	)
)

(cl-comment 'SFR-T2')

(forall (x y)
	(if	
		(sf_touches x y)
		(not (sf_point x))
	)
)

(forall (x y)
	(if	
		(sf_touches x y)
		(not (sf_point y))
	)
)


)