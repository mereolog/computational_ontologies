(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/gangemi-participation/gangemi.clif

(forall (x)
	(if	(Event x)
		(exists (y)
			(and	(Object x)
				(hasParticipant x y)))))

(forall (x)
	(if	(Object x)
		(exists (y)
			(and	(Event x)
				(hasParticipant y x)))))

(forall (x)
	(if	(Event x)
		(exists (y)
			(pre x y))))

(forall (x)
	(if	(Object x)
		(exists (y)
			(pre x y))))

(forall (x y)
	(if	(pre x y)
		(and	(or	(Object x)
				(Event x))
			(Time y))))

)
