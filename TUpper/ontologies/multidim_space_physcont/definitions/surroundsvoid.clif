(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_physcont/definitions/surroundsvoid.clif

(cl-comment 'detachable containment from 2013 COSIT paper')

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_space_physcont/definitions/surrounds.clif)

(cl-comment 'VSur-D')

(forall (x y)
	(iff
		(surroundsvoid y x)
		(and
			(surrounds y x)
			(V x)
		)
	)
)


)