(cl-text http://colore.oor.net/multidim_space_physcont/theorems/depcont_full_theorems.clif

(cl-imports http://colore.oor.net/multidim_space_physcont/definitions/osurroundsmat.clif)

(cl-imports http://colore.oor.net/multidim_space_physcont/definitions/osurroundsvoid.clif)

(cl-comment 'JEPD Theorems from end of Section 7.2 and from 7.3 of the COSIT containment paper')

(cl-comment 'JEPD17a')


(forall (x y)
	(if
		(surrounds y x)
			(or
				(surroundsvoid y x)
				(surroundsmat y x)
			)
		)
	)
)

(cl-comment 'JEPD17b')


(forall (x y)
	(if
		(surroundsvoid y x)
		(surrounds y x)
	)
)

(cl-comment 'JEPD17c')


(forall (x y)
	(if
		(surroundsmat y x)
		(surrounds y x)
	)
)



(cl-comment 'JEPD18')


(forall (x y)
	(if
		(surroundsmat y x)
		(not (surroundsvoid y x))
	)
)


(cl-comment 'JEPD19a')

(forall (x y)
	(if
		(and
			(or
				(mat x)
				(V x)
			)
			(or
				(mat y)
				(V y)
			)
		)
		(if
			(detcont y x)
			(or
				(inside x y)
				(surrounds y x)
			)
		)
	)
)



(cl-comment 'JEPD19b')

(forall (x y)
	(if
		(and
			(or
				(mat x)
				(V x)
			)
			(or
				(mat y)
				(V y)
			)
		)
		(if
			(or
				(inside x y)
				(surrounds y x)
			)
			(detcont y x)
		)
	)
)



(cl-comment 'JEPD20')


(forall (x y)
	(if
		(inside x y)
		(not (surrounds y x))
	)
)


)