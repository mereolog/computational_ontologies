(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/subgraph/half_strong.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/subgraph/graph_hom.clif)

(forall (p1 p2 l1 l2)
	(if	(and	(point p1)
			(point p2)
			(line l1)
			(line l2)
			(in p1 l1)
			(in p2 l2)
			(adj l1 l2))
		(exists (p3 p4)
			(and	(point p3)
				(point p4)
				(in p3 l1)
				(in p4 l2)
				(adj p3 p4)))))

)
