fof(axiom190, axiom, (! [X] : ! [Y] :  (sum(X,Y)=sum(Y,X)))).
fof(axiom200, axiom, (! [X] : ! [Y] :  ((lt(x,y) -> =(y,sum(x,y)))))).
fof(axiom210, axiom, (! [X] : ! [Y] : ! [Z] :  (((leq(x,y) & Cont(z,y)) -> Cont(z,sum(x,y)))))).
fof(axiom220, axiom, (! [X] : ! [Y] : ! [Z] :  (((Cont(z,sum(x,y)) & ~Cont(z,x)) -> Cont(difference(z,x),y))))).
