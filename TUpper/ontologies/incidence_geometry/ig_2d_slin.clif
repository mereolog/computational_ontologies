(cl-text http://colore.oor.net/incidence_geometry/ig_2d_slin.clif

(cl-comment 'semi-linear line spaces')

(cl-imports http://colore.oor.net/incidence_geometry/ig_2d.clif)

(cl-comment 'I.2a')

(forall (l m x y)
	(if
		(and
			(not (= x y))
			(L l)
			(L m)
			(on x l)
			(on y l)
			(on x m)
			(on y m)
		)
		(= l m)
	)
)

)