(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/mereological_bundle/m_bundle.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/weak_bipartite.clif)

(forall (x y p)
	(if	(tpart x y p)
		(and	(line x)
			(line y)
			(point p))))

(forall (x y p)
	(if	(tpart x y p)
		(and	(in x p)
			(in y p))))

(forall (x p)
	(if	(and	(line x)
			(point p)
			(in p x))
		(tpart x x p)))

(forall (x y z p)
        (if     (and    (tpart x y p)
                        (tpart y z p))
                (tpart x z p)))

)
