(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multigraphs/vertex_intersect.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multigraphs/pseudograph.clif)

(forall (p)
	(if	(vertex p)
		(exists (e1 e2 p1 p2)
			(and	(madj e1 p p1)
				(madj e2 p p2)
				(not (= p1 p2))))))

)
