(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/contact_algebras/Conn.clif

(cl-comment 'Con: Connection axiom (connection of an entity with its complement)')

(forall (x) 	
	(if 	(and 	
			(not (= x zero)) 
			(not (= x one))) 
		(exists (y)
			(and 	
				(complement x y)
				(c x y)
			)
		)
	)
)

)
