(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/contact_algebras/C5prime.clif

(cl-comment 'C5prime')

(forall (x z)
	(if
		(and
			(not
				(and
					(= x zero)
					(= z one)
				)
			)
			(not
				(and
					(= z zero)
					(= x one)
				)
			)
		)
		(iff
			(C z x)
			(or
				(= z (oc x))
				(not (= (meet z (oc x)) z))
			)
		)
	)
)

)