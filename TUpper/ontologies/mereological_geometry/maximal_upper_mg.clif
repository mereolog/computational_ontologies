(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/mereological_geometry/maximal_upper_mg.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/mereological_geometry/m_wmg.clif)

(forall (x l)
	(if	(and	(point x)
			(line l))
		(iff 	(in x l)
			(forall (y)
				(if	(part x y)
					(in y l))))))

)
