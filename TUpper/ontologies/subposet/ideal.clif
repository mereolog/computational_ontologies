(cl-text http://colore.oor.net/subposet/ideal.clif

(cl-imports http://colore.oor.net/subposet/lower_set.clif)

(forall (x y)
	(if	(and	(mereo_element x)
			(mereo_element y))
		(exists (z)
			(and	(leq x z)
				(leq y z)))))

)
