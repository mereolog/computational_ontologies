(cl-text http://colore.oor.net/multigeometry/multi_reflect.clif

(cl-imports http://colore.oor.net/multigeometry/multi_g.clif)

(forall (x y l1 l2)
	(if	(and	(leq l1 l2)
			(point x)
			(point y)
			(in x l1)
			(in y l2))
		(part x y)))

)
