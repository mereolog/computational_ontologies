(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/unique_triple_lines.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/bipartite_incidence/weak_bipartite.clif)

(forall (p1 p2 l1 l2 l3)
	(if	(and	(point p1)
			(line l1)
			(line l2)
			(line l3)
			(in p1 l1)
			(in p1 l2)
			(in p1 l3)
			(point p2)
			(in p2 l1)
			(in p2 l2))
		(or	(= p1 p2)
			(= l1 l2))))

)
