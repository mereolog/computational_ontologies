formulas(assumptions).

all s1 all s2(
    arboreal_subocc(a1,a2) -> arboreal(s1) & arboreal(s2)
).

all s(
    arboreal(s) -> arboreal_subocc(s,s)
).

all s1 all s2(
    arboreal_subocc(s1,s2) & arboreal_subocc(s2,s1) -> (s1=s2)
).

all s1 all s2 all s3(
    arboreal_subocc(s1,s2) & arboreal_subocc(s2,s3) -> arboreal_subocc(s1,s3)
).

all s1 all s2(
    arboreal_subocc(s1,s2) -> (s1=s2)
).

%definitions

all x(arboreal(x) <-> D(x)).

all x all y(
	arboreal_subocc(x,y) <-> part(x,y)
).

(all x all y
	(leq(x,y)
	->
	(D(x) & D(y)))).

% Reflexive
all x
	(D(x)
	->
	(leq(x,x))).

% Antisymmetry
all x all y
	((leq(x,y)
	& leq(y,x))
	->
	(x=y)).

% Transitive
all x all y all z
	((leq(x,y)
	& leq(y,z))
	->
	leq(x,z)).

(all x all y
	(lt(x,y)
	<->
	(leq(x,y)
	& (x != y)))).

%antichain
(all x all y
	(leq(x,y)
	->
	(x=y))).

end_of_list.

formulas(goals).

% Transitive
all x all y all z
	((leq(x,y)
	& leq(y,z))
	->
	leq(x,z)).

end_of_list.

