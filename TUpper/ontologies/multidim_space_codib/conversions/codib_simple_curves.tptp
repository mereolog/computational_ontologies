fof(axiom140, axiom, (! [X] : ! [Y] : ! [Z] :  (((SimpleCurve(x) & Cont(y,x) & Cont(z,x) & ~=(y,z)) -> \exists v\;[(SimpleCurveSegment(v) & P(v,x) & BCont(y,v) & BCont(z,v))])))).
fof(axiom150, axiom, (! [X] :  ((SimpleCurve(x) <-> (SimpleCurveSegment(x) | SimpleLoopCurve(x) | SimpleRayCurve(x) | SimpleInfiniteCurve(x)))))).
