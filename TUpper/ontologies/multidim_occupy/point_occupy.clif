(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_occupy/point_occupy.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_mereotopology_codi/definitions/point_region.clif)
(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/shape/shape.clif)


(forall (x y) 
    (if (point_occupies x y) 
        (and (PointRegion y)
		     (point x))))

(forall (x y z) 
    (if (and	(point_occupies x y)
				(point_occupies x z))
        (= y z)))			 
        
                        
(forall (x)   
    (if (box x) 
        (exists (y) (point_occupies x y))))

(forall (x y r1 r2) 
    (if (and	(point_C x y)
				(point_occupies x r1)
				(point_occupies y r2))
        (C r1 r2)))			         
        
)
