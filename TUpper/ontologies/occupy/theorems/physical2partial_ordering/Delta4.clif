(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/occupy/theorems/physical2partial_ordering/Delta4.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/occupy/physical_mereology/physical_mereology.clif)

(forall (x)
	(iff	(= x x)
		(physical_body x)))

(forall (x)
	(iff	(leq x y)
		(physical_part x y)))

)
