(cl-text https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_mereotopology_codi/definitions/ppo.clif

(cl-imports https://raw.githubusercontent.com/mereolog/computational_ontologies/main/TUpper/multidim_mereotopology_codi/definitions/po.clif)

(cl-comment 'PPO-D: proper partial overlap as overlap where neither of the involved entity is part of the other.')

(forall (x y)
	(iff	
		(PPO x y)
		(and
			(PO x y)
			(not (P x y))
			(not (P y x))
		)
	)
)

)


