(cl-text  https://raw.githubusercontent.com/gruninger/colore/master/ontologies/owltime_eventuality/eventuality.clif

(cl-imports  https://raw.githubusercontent.com/gruninger/colore/master/ontologies/owltime_inside/owltime_inside.clif)

(forall (e t)
	(if	(atTime e t)
		(Instant t)))

(forall (e t)
	(if	(during e t)
		(Interval t)))

(forall (e t1 t2)
	(if	(and	(during e t1)
			(inside t2 t1))
		(atTime e t2)))

(forall (e t t1)
	(if	(and	(during e t)
			(intDuring t1 t))
		(during e t1)))


)
